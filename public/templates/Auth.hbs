<div>
  <button class="cancel-button">
    <img src="./static/svg/cancel.svg" alt="Cancel">
  </button>
  <img class="ibackground" src="static/img/image_2.png" alt="Background Image">
  <div class="form-container">
    <form class="input-container">
      <h4 class="auth-title">Войти в PinSpire</h4>
      {{> labeledInput label="Имя пользователя" type="text" id="username"}}
      {{> labeledInput label="Пароль" type="password" id="password"}}
      <button type="submit" class="button">Войти</button>
      <p class="already-registered">Все еще нет аккаунта? <a href="#">Регистрация</a></p>
    </form>
  </div>
</div>
